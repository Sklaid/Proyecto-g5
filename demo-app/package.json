{
  "name": "aiops-demo-app",
  "version": "1.0.0",
  "description": "Aplicacion demo con OpenTelemetry instrumentation para plataforma AIOps",
  "main": "src/index.js",
  "scripts": {
    "start": "node -r ./src/tracing.js src/index.js",
